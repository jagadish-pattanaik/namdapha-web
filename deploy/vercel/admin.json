{
  "name": "project-s-admin",
  "version": 2,
  "builds": [
    {
      "src": "backend/admin-dashboard/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/admin-dashboard/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-api-domain.railway.app"
  }
}